#!/bin/bash

if test -f /workspaces/github/script/setup-copilot; then
  echo "Setting up copilot..."
  /workspaces/github/script/setup-copilot
fi

if test -f /workspaces/github/bin/toggle-feature-flag; then
  echo "Setting copilot-custom-models feature flag..."
  /workspaces/github/bin/toggle-feature-flag enable copilot-custom-models
fi